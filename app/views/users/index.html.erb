<% if notice %>
  <p id="notice"><%= notice %></p>
<% end %>

<h1>Admin Users</h1>

<table>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td>-</td>
        <td><%= user.username %></td>
        <td><%= link_to 'Edit Information', edit_user_path(user) %></td>
        <td><%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create Admin User', new_user_path %>
